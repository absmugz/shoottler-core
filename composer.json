{
    "name": "shoottler/shoottler-core",
    "description": "Manage any kind of bookable resources",
    "keywords": [
        "booking",
        "boilerplate",
        "laravel",
        "vue"
    ],
    "license": "GPL-3.0",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "cybercog/laravel-eloquent-flag": "^4.0",
        "fideloper/proxy": "^4.0",
        "grimzy/laravel-mysql-spatial": "^2.1",
        "guzzlehttp/guzzle": "^6.3",
        "haydenpierce/class-finder": "^0.2.0",
        "joshbrw/laravel-module-installer": "^0.1.4",
        "laracasts/flash": "^3.0",
        "laravel/framework": "5.7.*",
        "laravel/passport": "^7.0",
        "laravel/tinker": "^1.0",
        "nanigans/single-table-inheritance": "0.8.*",
        "nwidart/laravel-modules": "^4.0",
        "shoottler/item-module": "dev-master",
        "sofa/eloquence": "^5.6",
        "spatie/laravel-activitylog": "^3.1",
        "spatie/laravel-medialibrary": "^7.0.0"
    },
    "require-dev": {
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/",
            "Modules\\": "Modules/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
